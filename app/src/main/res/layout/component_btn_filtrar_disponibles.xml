<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent">    <ImageView        android:id="@+id/btn_filtrar_disponibles"        android:layout_width="@dimen/comp_btn_filtrar_disp_img_width"        android:layout_height="@dimen/comp_btn_filtrar_disp_img_height"        android:contentDescription="@string/content_description"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:srcCompat="@drawable/icon_map_btn_filter_pool_light" />    <TextView        android:id="@+id/tv_filtrar_disponibles"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:fontFamily="@font/nunito_semi_bold"        android:gravity="center"        android:text="@string/component_btn_filtrar_disponibles_filter_available"        android:textColor="@color/fragment_map_filter_pool_dark_txt"        android:textSize="@dimen/comp_btn_filtrar_disp_txtsize"        app:layout_constraintBottom_toBottomOf="@+id/btn_filtrar_disponibles"        app:layout_constraintEnd_toEndOf="@+id/btn_filtrar_disponibles"        app:layout_constraintStart_toStartOf="@+id/btn_filtrar_disponibles"        app:layout_constraintTop_toTopOf="@+id/btn_filtrar_disponibles" /></androidx.constraintlayout.widget.ConstraintLayout>