<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="wrap_content"    android:paddingEnd="@dimen/comp_term_cond_padend"    android:layout_height="wrap_content"    tools:ignore="RtlSymmetry">    <androidx.appcompat.widget.LinearLayoutCompat        android:id="@+id/text_terms_and_conditions"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:orientation="horizontal"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent">        <androidx.appcompat.widget.LinearLayoutCompat            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:gravity="center"            android:orientation="vertical"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent"            tools:ignore="UseSwitchCompatOrMaterialXml">            <androidx.appcompat.widget.SwitchCompat                android:id="@+id/accept_terms_switch"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:thumb="@drawable/bg_switch_thumb"                app:track="@drawable/bg_switch_track" />        </androidx.appcompat.widget.LinearLayoutCompat>        <androidx.appcompat.widget.LinearLayoutCompat            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:layout_marginStart="@dimen/comp_term_cond_alltxt_marginstart"            android:orientation="horizontal"            android:paddingTop="@dimen/comp_term_cond_txtwrap_padtop"            android:paddingBottom="@dimen/comp_term_cond_txtwrap_padbottom"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_vertical"                android:layout_marginEnd="@dimen/comp_term_cond_txtaccept_marginend"                android:fontFamily="@font/nunito_bold"                android:text="@string/component_terms_and_cond_accept"                android:textColor="@color/terms_and_cond_text_accept"                android:textSize="@dimen/comp_term_cond_txtsize"                tools:ignore="SmallSp" />            <TextView                android:id="@+id/tv_textview"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center_vertical"                android:fontFamily="@font/nunito_bold"                android:text="@string/component_terms_and_cond_read"                android:textColor="@color/terms_and_cond_text_termcond"                android:textSize="@dimen/comp_term_cond_txtsize"                tools:ignore="SmallSp" />        </androidx.appcompat.widget.LinearLayoutCompat>    </androidx.appcompat.widget.LinearLayoutCompat></androidx.constraintlayout.widget.ConstraintLayout>