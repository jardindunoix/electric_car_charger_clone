<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent">    <ImageView        android:id="@+id/img_settings"        android:layout_width="@dimen/comp_bottom_menu_img_width"        android:layout_height="@dimen/comp_bottom_menu_img_height"        android:contentDescription="@string/content_description"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:srcCompat="@drawable/icon_bottom_menu_btn_config_light" />    <TextView        android:id="@+id/tv_settings"        android:layout_width="0dp"        android:layout_height="wrap_content"        android:layout_marginBottom="@dimen/comp_bottom_menu_settings_txt_marginbottom"        android:fontFamily="@font/nunito_semi_bold"        android:gravity="center"        android:text="@string/menu_bottom_settings"        android:textColor="@color/bottom_menu_txt_config_dark"        android:textSize="@dimen/comp_bottom_menu_settings_txtsize"        app:layout_constraintBottom_toBottomOf="@+id/img_settings"        app:layout_constraintEnd_toEndOf="@+id/img_settings"        app:layout_constraintStart_toStartOf="@+id/img_settings" /></androidx.constraintlayout.widget.ConstraintLayout>