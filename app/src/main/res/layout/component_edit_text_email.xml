<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent">    <EditText        android:id="@+id/edit_text_email"        android:layout_width="@dimen/comp_et_email_et_width"        android:layout_height="@dimen/comp_et_email_et_height"        android:autofillHints=""        android:background="@drawable/bg_btn_round_corner_pressed"        android:backgroundTint="@color/edit_text_bg_tint"        android:fontFamily="@font/nunito_regular"        android:hint="@string/component_edit_text_email_hint"        android:inputType="textEmailAddress"        android:paddingStart="@dimen/comp_et_email_padstart"        android:paddingEnd="@dimen/comp_et_email_padend"        android:textColor="@color/edit_text_text_color"        android:textColorHint="@color/edit_text_text_color_hint"        android:textSize="@dimen/comp_et_email_txtsize"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        tools:ignore="RtlSymmetry" /></androidx.constraintlayout.widget.ConstraintLayout>