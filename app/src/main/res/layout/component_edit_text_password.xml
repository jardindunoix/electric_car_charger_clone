<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent">    <EditText        android:id="@+id/edit_text_password"        android:layout_width="@dimen/comp_et_pass_et_width"        android:layout_height="@dimen/comp_et_pass_et_height"        android:autofillHints=""        android:background="@drawable/bg_btn_round_corner_pressed"        android:fontFamily="@font/nunito_regular"        android:hint="@string/component_edit_text_password_hint"        android:inputType="textPassword"        android:letterSpacing="0.3"        android:backgroundTint="@color/edit_text_bg_tint"        android:paddingStart="@dimen/comp_et_pass_padstart"        android:paddingEnd="@dimen/comp_et_pass_padend"        android:textColor="@color/edit_text_text_color"        android:textColorHint="@color/edit_text_text_color_hint"        android:textSize="@dimen/comp_et_pass_txtsize"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        tools:ignore="RtlSymmetry,TextFields" />    <include        android:id="@+id/edit_text_password_icon_hide_include"        layout="@layout/component_icon_eye"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        app:layout_constraintBottom_toBottomOf="@+id/edit_text_password"        app:layout_constraintEnd_toEndOf="@+id/edit_text_password"        app:layout_constraintHorizontal_bias="0.99"        app:layout_constraintStart_toStartOf="@+id/edit_text_password"        app:layout_constraintTop_toTopOf="@+id/edit_text_password"        app:layout_constraintVertical_bias="0.504" /></androidx.constraintlayout.widget.ConstraintLayout>