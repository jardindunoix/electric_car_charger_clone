<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent">    <TextView        android:id="@+id/welcome_text"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:fontFamily="@font/nunito_medium"        android:gravity="center"        android:paddingTop="@dimen/comp_welcome_padtop"        android:paddingBottom="@dimen/comp_welcome_padbottom"        android:text="@string/component_welcome_text"        android:textColor="@color/welcome_text"        android:textSize="@dimen/comp_welcome_txtsize"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" /></androidx.constraintlayout.widget.ConstraintLayout>