<?xml version="1.0" encoding="utf-8"?><selector xmlns:android="http://schemas.android.com/apk/res/android">    <item android:state_checked="false">        <shape android:shape="rectangle">            <solid android:color="@color/bg_switch_custom_track_checked_false_solid" />            <stroke android:width="@dimen/draw_switch_track_shape_width" android:color="@color/bg_switch_custom_track_checked_false_stroke" />            <corners android:radius="@dimen/draw_switch_track_radius" />        </shape>    </item>    <item android:state_checked="true">        <shape android:shape="rectangle">            <solid android:color="@color/bg_switch_custom_track_checked_true_solid" />            <stroke android:width="@dimen/draw_switch_track_shape_width" android:color="@color/bg_switch_custom_track_checked_true_stroke" />            <corners android:radius="@dimen/draw_switch_track_radius" />        </shape>    </item></selector>